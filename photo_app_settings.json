{
  "skip_unchanged_photos": true,
  "use_exif_for_date": true,
  "dark_mode": false,
  "thumbnail_cache_enabled": false,
  "cache_size_mb": 500,
  "show_decoder_warnings": true,
  "db_debug_logging": false,
  "show_sql_queries": false,
  "use_cache_warmup": true,
  "cache_auto_cleanup": true,
  "meta_workers": 4,
  "meta_timeout_secs": 8.0,
  "meta_batch": 200,
  "auto_run_backfill_after_scan": true,
  "show_backfill_panel": false,
  "sidebar_folded": false,
  "sidebar_mode": "list",
  "ignore_folders": [
    "AppData",
    "Program Files",
    "Program Files (x86)",
    "Windows",
    "$Recycle.Bin",
    "System Volume Information",
    "__pycache__",
    "node_modules",
    "Temp",
    "Cache",
    "Microsoft",
    "Installer",
    "Recovery",
    "Logs",
    "ThumbCache",
    "ActionCenterCache"
  ]
}